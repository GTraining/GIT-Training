<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jason.jason_workshop_3">

    <application
        android:allowBackup="true"
        android:icon="@drawable/heart_pulse"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.UserMainView.UserMainActivity" />
        <activity android:name=".View.FeatureView.DailyFitnessActivity" />
        <activity android:name=".View.FeatureView.DailyDrinkActivity" />
        <activity android:name=".View.FeatureView.DailyDietActivity" />
        <activity android:name=".View.LoginView.SignupActivity" />
        <activity android:name=".View.FeatureView.MonthlyCheckBMIActivity" />

        <receiver android:name=".Alarm.AlarmReceiver" />

        <activity android:name=".View.FeatureView.BMIChartActivity" />
        <activity android:name=".View.LoginView.LoginActivity" />
    </application>

</manifest>
