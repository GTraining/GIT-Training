<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Button" parent="Theme.AppCompat.Light">
        <item name="android:textColor">@color/white</item>
        <item name="colorControlHighlight">@color/backgroundmenupress</item>
        <item name="colorButtonNormal">@color/backgroundmenu</item>
    </style>
    <style name="Button_notification" parent="Theme.AppCompat.Light">
    <item name="android:textColor">@color/white</item>
    <item name="colorControlHighlight">@color/text</item>
    <item name="colorButtonNormal">@color/white</item>
</style>

</resources>
